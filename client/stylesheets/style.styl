  box-shadow: 0px 1px 1px rgba(0, 0, 0, 0.075) inset
  background: #FFF url("image/icon-search.png") no-repeat scroll 11px 11px
  &:focus
    border-color: #B1B1B1


.search-input:active{
  outline: none;
}

.search-box{
  z-index: 3;
  position: absolute;
  box-shadow: 0px 1px 6px rgba(0, 0, 0, 0.35);
  border-radius: 4px;
}
.search-box:first:child{
  border-top-right-radius: 4px;
  border-top-left-radius: 4px;
}
.search-date{
  font-weight: bold;
  display: inline-block;
float: right;
}
.search-more{
border: medium none;
background: transparent none repeat scroll 0% 0%;
display: block;
width: 100%;
font-weight: bold;
font-size: 20px;
color: #09AEF2;
transition: all 0.1s;
}
.search-more:hover{
